<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
  <key>familyName</key>
  <string>Wavefont</string>
  <key>styleName</key>
  <string>Thin</string>
  <key>postscriptFontName</key>
  <string>Wavefont-Thin</string>
  <key>postscriptFullName</key>
  <string>Wavefont Thin</string>
  <key>postscriptWeightName</key>
  <string>Thin</string>
  <key>versionMajor</key>
  <integer>{{ font.version.major }}</integer>
  <key>versionMinor</key>
  <integer>{{ font.version.minor }}</integer>
  <key>copyright</key>
  <string>Copyright 2022 The Wavefont Project Authors (https://github.com/dy/wavefont)</string>
  <key>unitsPerEm</key>
  <integer>{{upm font.max }}</integer>
  <key>ascender</key>
  <integer>{{upm font.ascender }}</integer>
  <key>descender</key>
  <integer>-{{upm font.descender }}</integer>
  <key>xHeight</key>
  <integer>0</integer>
  <key>capHeight</key>
  <integer>0</integer>
  <key>postscriptUnderlinePosition</key>
  <integer>0</integer>
  <key>postscriptUnderlineThickness</key>
  <integer>50</integer>
  <key>postscriptIsFixedPitch</key>
  <true/>

  <key>openTypeNameLicense</key>
  <string>This Font Software is licensed under the SIL Open Font License, Version 1.1. This license is available with a FAQ at: https://scripts.sil.org/OFL</string>
  <key>openTypeNameLicenseURL</key>
  <string>https://scripts.sil.org/OFL</string>
  <key>openTypeOS2VendorID</key>
  <string>DY  </string>
  <key>openTypeOS2Selection</key>
  <array>
    <integer>7</integer>
  </array>
  <key>openTypeOS2WeightClass</key>
  <integer>{{ weight }}</integer>
  <key>openTypeOS2Type</key>
  <array>
  </array>

  <key>openTypeHheaAscender</key>
  <integer>{{upm font.ascender }}</integer>
  <key>openTypeHheaDescender</key>
  <integer>-{{upm font.descender }}</integer>
  <key>openTypeHheaCaretOffset</key>
  <integer>0</integer>
  <key>openTypeHheaLineGap</key>
  <integer>0</integer>

  <key>openTypeOS2TypoAscender</key>
  <integer>{{upm font.ascender }}</integer>
  <key>openTypeOS2TypoDescender</key>
  <integer>-{{upm font.descender }}</integer>
  <key>openTypeOS2TypoLineGap</key>
  <integer>0</integer>

  <key>openTypeOS2TypoLineGap</key>
  <integer>0</integer>
  <key>openTypeOS2WinAscent</key>
  <integer>{{upm font.ascender }}</integer>
  <key>openTypeOS2WinDescent</key>
	<integer>{{upm font.descender }}</integer>

  <key>openTypeOS2Panose</key>
  <array>
		<integer>2</integer>
		<integer>0</integer>
		<integer>0</integer>
		<integer>9</integer>
		<integer>0</integer>
		<integer>0</integer>
		<integer>0</integer>
		<integer>0</integer>
		<integer>0</integer>
		<integer>0</integer>
  </array>
  </dict>
</plist>
